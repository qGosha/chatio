{
  "name": "fullstack",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "node server/server.js",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix server/client && npm run build --prefix server/client",
    "startdev": "export NODE_ENV=development || SET \"NODE_ENV=development\" && nodemon server/server.js",
    "startdevd": "export NODE_ENV=development || SET \"NODE_ENV=development\" && node --inspect-brk server/server.js",
    "client": "npm run start --prefix server/client",
    "dev": "concurrently \"npm run startdev\" \"npm run client\"",
    "devbrk": "concurrently \"npm run startdevd\" \"npm run client\"",
    "test": "NODE_ENV=test || SET \"NODE_ENV=test\" && nodemon --exec mocha -R \"./server/test/*.js\"",
    "seed": "NODE_ENV=seed || SET \"NODE_ENV=seed\" && node \"./server/test/seed.js\"",
    "seedbrk": "NODE_ENV=seed || SET \"NODE_ENV=seed\" && node --inspect-brk ./server/test/seed.js"
  },
  "engines": {
    "node": "10.5.0"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.18.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "cloudinary": "^1.13.2",
    "concurrently": "^4.0.1",
    "connect-mongo": "^2.0.1",
    "cors": "^2.8.5",
    "express": "^4.16.3",
    "express-form-data": "^2.0.8",
    "express-session": "^1.15.6",
    "express-socket.io-session": "^1.3.5",
    "lodash": "^4.17.15",
    "lodash.omit": "^4.5.0",
    "lodash.pick": "^4.4.0",
    "moment": "^2.22.2",
    "mongodb": "^3.1.4",
    "mongoose": "^5.2.12",
    "nodemailer": "^4.6.8",
    "passport": "^0.4.0",
    "passport-facebook": "^2.1.1",
    "passport-google-oauth20": "^1.0.0",
    "passport-local": "^1.0.0",
    "react": "^16.8.0-alpha.1",
    "react-dom": "^16.8.0-alpha.1",
    "react-images-upload": "^1.2.5",
    "socket.io": "^2.1.1",
    "validator": "^13.7.0"
  },
  "devDependencies": {
    "assert": "^1.4.1",
    "eslint": "^5.13.0",
    "eslint-config-google": "^0.12.0",
    "eslint-config-prettier": "^4.0.0",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "faker": "^4.1.0",
    "mocha": "^6.2.0",
    "remote-redux-devtools": "^0.5.13"
  }
}
