    "cloudinary": "^1.13.2",
    "concurrently": "^4.0.1",
    "connect-mongo": "^2.0.1",
    "cors": "^2.8.5",
    "express": "^4.16.3",
    "express-form-data": "^2.0.7",
    "express-session": "^1.15.6",
    "express-socket.io-session": "^1.3.5",
    "lodash": "^4.17.11",
    "moment": "^2.22.2",
    "mongodb": "^3.1.4",
    "mongoose": "^5.2.12",
    "nodemailer": "^4.6.8",
